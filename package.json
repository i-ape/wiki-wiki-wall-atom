{
  "name": "wiki-wiki-wall",
  "main": "./lib/wiki-wiki-wall",
  "version": "0.1.0",
  "author": "Dirk Elmendorf",
  "description": "This plugin for Atom add support for the WikiWikiWall language and rendering of the SVGS",
  "keywords": [],
  "NOnactivationCommands": {
    "atom-workspace": "wiki-wiki-wall:toggle"
  },
  "scripts": {
    "prettier": "echo 'Applying prettier....' && prettier --write \"lib/**/*.js\" > /dev/null  && exit 0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getAutoCompleteProviders"
      }
    }
  },
  "repository": "https://github.com/r26D/wiki-wiki-wall-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-message-panel": "^1.3.0",
    "bytes": "^2.4.0",
    "etch": "0.9.0",
    "fs-plus": "^3.0.0",
    "lodash": "^4.17.21"
  },
  "deserializers": {
    "ImageEditor": "deserialize"
  }
}
